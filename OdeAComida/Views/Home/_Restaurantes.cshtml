@model IPagedList<ListViewModelRestaurante>

    <div id="listaRestaurantes">

        <div class="pagedList" data-otf-target="#listaRestaurantes">
            @Html.PagedListPager(Model, page => Url.Action("Index", new { page }),
 PagedListRenderOptions.MinimalWithItemCountText)
        </div>

        @foreach (var item in Model)
        {<div>
                <h4>@item.Nome</h4>
                <div>@item.Cidade, @item.Pais</div>
                <div>Análises: @item.QtdAnalises</div>
                <hr />
            </div>
        }
    </div>
